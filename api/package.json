{
  "name": "webupload-api",
  "version": "1.0.0",
  "description": "Secure File Upload and Management API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts | pino-pretty",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "worker": "node dist/worker.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "migrate:up": "node dist/scripts/migrate-up.js",
    "migrate:down": "node dist/scripts/migrate-down.js",
    "seed": "node dist/scripts/seed.js"
  },
  "keywords": [
    "file",
    "upload",
    "secure",
    "s3",
    "minio",
    "clamav",
    "fastify"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.433.0",
    "@aws-sdk/s3-request-presigner": "^3.433.0",
    "@fastify/auth": "^4.3.0",
    "@fastify/cookie": "^9.1.0",
    "@fastify/cors": "^8.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/jwt": "^7.2.1",
    "@fastify/rate-limit": "^8.0.3",
    "@fastify/swagger": "^8.9.0",
    "@fastify/swagger-ui": "^1.10.0",
    "@fastify/websocket": "^8.2.0",
    "argon2": "^0.31.1",
    "bullmq": "^4.11.0",
    "clamscan": "^2.1.2",
    "dotenv": "^16.3.1",
    "fastify": "^4.23.2",
    "fastify-plugin": "^4.5.1",
    "file-type": "^16.5.4",
    "ioredis": "^5.3.2",
    "mime-types": "^2.1.35",
    "pg": "^8.11.3",
    "pino": "^8.15.6",
    "sharp": "^0.32.6",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/mime-types": "^2.1.2",
    "@types/node": "^20.8.2",
    "@types/pg": "^8.10.3",
    "@types/uuid": "^9.0.5",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.4.2",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "pino-pretty": "^10.2.3",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
